import java.util.*;

import javax.swing.plaf.synth.SynthLookAndFeel;

public class patterns2 {
    public static void main(String[] args) {
        int n = 6;
        int m = 7;
        for (int i = 1; i <= n; i++) {
            for (int j = 1; j <= m; j++) {
                System.out.print("*");
            }
            System.out.println();
            }
    }
}    
